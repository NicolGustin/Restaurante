      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Ingredientes\Index.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\Ingredientes\Index.cshtml" />
    <_ContentIncludedByDefault Remove="Pages\Pedido\Pedido.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Pages\Ingredientes\Index.cshtml" />
    <None Include="Pages\Pedido\Pedido.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Pedido\Pedido.cshtml" />
